{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
      "format": "[%(levelname)s|%(module)s|L%(lineno)d] %(asctime)s: %(message)s",
      "datefmt": "%Y-%m-%dT%H:%M:%S%z"
    },
    "json": {
      "()": "logger.MyJSONFormatter",
      "fmt_keys": {
        "level": "levelname",
        "message": "message",
        "timestamp": "timestamp",
        "logger": "name",
        "module": "module",
        "function": "funcName",
        "line": "lineno",
        "thread_name": "threadName"
      }
    }
  },
  "handlers": {
    "stderr": {
      "class": "logging.StreamHandler",
      "level": "WARNING",
      "formatter": "simple",
      "stream": "ext://sys.stderr"
    },
    "file_json": {
      "class": "logging.handlers.RotatingFileHandler",
      "level": "ERROR",
      "formatter": "json",
      "filename": "log.jsonl",
      "maxBytes": 104857600,
      "backupCount": 3
    },
    "queue_handler": {
      "class": "logging.handlers.QueueHandler"
    }
  },
  "loggers": {
    "root": {
      "level": "WARNING",
      "handlers": ["queue_handler", "stderr", "file_json"]
    }
  }
}
